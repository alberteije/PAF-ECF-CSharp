<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="PafEcf" namespace="PafEcf.DTO" default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
  <class name="UnidadeProdutoDTO" table="unidade_produto" >

    <id name="Id" column="ID">
      <generator class="sequence">
        <param name="sequence">unidade_produto</param>
      </generator>
    </id>


    <property name="Sigla" column="SIGLA" />
    <property name="Descricao" column="DESCRICAO" />
    <property name="PodeFracionar" column="PODE_FRACIONAR" />
  </class>
</hibernate-mapping>