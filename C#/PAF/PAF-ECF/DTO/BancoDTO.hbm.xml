<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="PafEcf" namespace="PafEcf.DTO" default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
  <class name="BancoDTO" table="banco" >

    <id name="Id" column="ID">
      <generator class="sequence">
        <param name="sequence">banco</param>
      </generator>
    </id>

    <property name="Codigo" column="CODIGO" />
    <property name="Nome" column="NOME" />
    <property name="Url" column="URL" />
  </class>
</hibernate-mapping>