<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="PafEcf" namespace="PafEcf.DTO" default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
  <class name="EcfDocumentosEmitidosDTO" table="ecf_documentos_emitidos" >

    <id name="Id" column="ID">
      <generator class="sequence">
        <param name="sequence">ecf_documentos_emitidos</param>
      </generator>
    </id>


    <property name="IdEcfMovimento" column="ID_ECF_MOVIMENTO" />
    <property name="DataEmissao" column="DATA_EMISSAO" />
    <property name="HoraEmissao" column="HORA_EMISSAO" />
    <property name="Tipo" column="TIPO" />
    <property name="Coo" column="COO" />
  </class>
</hibernate-mapping>