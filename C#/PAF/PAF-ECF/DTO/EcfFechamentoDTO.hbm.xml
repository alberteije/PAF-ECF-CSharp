<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="PafEcf" namespace="PafEcf.DTO" default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
  <class name="EcfFechamentoDTO" table="ecf_fechamento" >

    <id name="Id" column="ID">
      <generator class="sequence">
        <param name="sequence">ecf_fechamento</param>
      </generator>
    </id>


    <property name="IdEcfMovimento" column="ID_ECF_MOVIMENTO" />
    <property name="TipoPagamento" column="TIPO_PAGAMENTO" />
    <property name="Valor" column="VALOR" />
  </class>
</hibernate-mapping>