<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="PafEcf" namespace="PafEcf.DTO" default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
  <class name="EcfContadorDTO" table="ecf_contador" >

    <id name="Id" column="ID">
      <generator class="sequence">
        <param name="sequence">ecf_contador</param>
      </generator>
    </id>


    <property name="Cpf" column="CPF" />
    <property name="Cnpj" column="CNPJ" />
    <property name="Nome" column="NOME" />
    <property name="InscricaoCrc" column="INSCRICAO_CRC" />
    <property name="Fone" column="FONE" />
    <property name="Fax" column="FAX" />
    <property name="Logradouro" column="LOGRADOURO" />
    <property name="Numero" column="NUMERO" />
    <property name="Complemento" column="COMPLEMENTO" />
    <property name="Bairro" column="BAIRRO" />
    <property name="Cep" column="CEP" />
    <property name="CodigoMunicipio" column="CODIGO_MUNICIPIO" />
    <property name="Uf" column="UF" />
    <property name="Email" column="EMAIL" />
  </class>
</hibernate-mapping>