<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="PafEcf" namespace="PafEcf.DTO" default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
  <class name="EcfTurnoDTO" table="ecf_turno" >

    <id name="Id" column="ID">
      <generator class="sequence">
        <param name="sequence">ecf_turno</param>
      </generator>
    </id>


    <property name="Descricao" column="DESCRICAO" />
    <property name="HoraInicio" column="HORA_INICIO" />
    <property name="HoraFim" column="HORA_FIM" />
  </class>
</hibernate-mapping>